<h1>Song Details</h1>

<div>Title: <%=@song.title%></div><br>
<div>Released? <%=@song.released%></div><br>
<div>Release Year: <%=@song.release_year%></div><br>
<div>Artist: <%=@song.artist_name%></div><br>
<div>Genre: <%=@song.genre%></div><br>

<%=form_tag @song, method: "DELETE" do %>
  <%=submit_tag "Delete this Song" %>
<% end %>

<%= link_to "Songs Index Page", songs_path %>
